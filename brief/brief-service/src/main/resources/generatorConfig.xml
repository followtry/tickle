<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <classPathEntry location="D:\dev_tools\dev_libs\.m2\repository\mysql\mysql-connector-java\5.1.37\mysql-connector-java-5.1.37.jar" />
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <!-- 去除自动生成的注释 -->
        <commentGenerator>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        	 connectionURL="jdbc:mysql://172.20.19.200:3306/test" userId="worktime" password="worktime">
        </jdbcConnection>

        <javaTypeResolver>
        	<!-- 是否强制转换Decimal或number域为java的BigDecimal -->
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

		<!-- java domain 位置,targetProject可以写fs位置 -->
        <javaModelGenerator targetPackage="cn.jingzz.brief.dao.model"
            targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <!-- setter方法是否过滤空字符 -->
            <property name="trimStrings" value="false" />
        </javaModelGenerator>

		<!-- 生成的映射xml位置 -->
        <sqlMapGenerator targetPackage="cn.jingzz.brief.dao.mapper"
            targetProject="src/main/resources">
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>

		<!-- 生成映射mapper接口位置 -->
       <javaClientGenerator type="XMLMAPPER"
            targetPackage="cn.jingzz.brief.dao.mapper"
            targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>
        
        <!-- 需要自动逆向生成代码的表 -->
        <!-- alias="heheh" => column="heheh_id" 添加列的前缀 
        	enableInsert="false" #是否生成insert方法，默认为true
        	enableDeleteByPrimaryKey="false"  #是否生成Delete方法，默认为true
        	enableUpdateByPrimaryKey="false"  #是否生成Update方法，默认为true
        	enableSelectByPrimaryKey="false"  #是否生成Select方法，默认为true
         -->
        <table tableName="test_mybatis"  domainObjectName="MyBatis" 
        schema=""
        	enableCountByExample="false"
            enableUpdateByExample="false" 
            enableDeleteByExample="false"
            enableSelectByExample="false" 
            selectByExampleQueryId="false" >
            
            <!-- 继承的父类 -->
            <!-- <property name="rootClass" value="cn.jingzztech.prac.mybatis.bean.BaseBean" /> -->
            <!-- 生成全属性参数构造方法和无参构造方法 -->
            <!-- <property name="constructorBased" value="false" /> -->
            <!-- 实现的接口 -->
            <!-- <property name="rootInterface" value="cn.jingzztech.prac.mybatis.bean.BaseBeaninterface" /> -->
            <!-- 是否使用Table字段名作为类的属性名 -->
            <property name="useActualColumnNames" value="false" />
            <!-- <generatedKey column="id" sqlStatement="mysql" identity="true" /> -->
            <!-- 配置忽略的列 -->
            <!-- <ignoreColumn column="test_time" /> -->
            <columnOverride column="LONG_VARCHAR_FIELD" jdbcType="VARCHAR" />
        </table>
        
    </context>
</generatorConfiguration>